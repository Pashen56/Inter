MyFoo(x,y){
return x*y;
}
MyFoo(7,8);
//Начальная проверка области видимости
Enot = 9;
{
out Enot;
a = 10;
out a;
{
a = 12;
out a;
{
b = 7;
}
out b;
}
out a;
}

/*
 Этот файл или же этот код нужен для проверки. 
 Все ли работает как надо или нет. 
 Если все работает, то этот текст исчезнет в новом файле (хнык). 
*/

// а тут инструкции, которые бы хорошо было бы и выполнить, ну конечно, посыле урезки комментариев

add 1, 5; // Тут идет сложение 1 + 5 = 6

in a, 6; // А здесь в переменную а заносится значение 6!
out a;

mul 3, a; // Ответ на это умножение -> 18

// 15 инструкций надо все написать и для 3 адресных 3 переменные попробовать

//Присваиваем данные
in hello, 5;
in world, 6;

//Вывод данных
out hello;
out world;

/*Дале будут производиться
различные операции
*/
add hello, 7;
add new, hello, world;
out new;

//Некоторые логические операции
and hello, 10;
or new, world;

mov world, hello;
out world;

add 2, 3; //тут

a = (3 + 4) * (5 - 1) - 4;
out a;

b = (4 - 3) * 23 + 10;
out b;

с = (world - 3) * 23 + 10;
out с;

// Будет ли работать так:
D4 = 4;
out D4;

// Тут дальше проверка функций всяких и видимостей 

foo(x,y){
return x+y;
}
foo(31,25);
